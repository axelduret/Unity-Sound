<!DOCTYPE html>
<!-- Html -->
<html
	lang="en">
	<!-- Head -->
	{% block head %}{% endblock head %}
	<!-- ./Head -->
	<!-- Body -->
	<body>
		<div class="screenLeft"></div>
		<!-- Main -->
		<div
			class="main">
			<!-- Sidebar -->
			{% include 'sidebar.twig' %}
			<!-- ./Sidebar -->
			<!-- Page -->
				<div
				class="page"> <!-- Header -->
				{% include 'header.twig' %}
				<!-- ./Header -->
				<!-- Main Content -->{% block content %}
				{% endblock content %}
				<!-- ./Main Content -->
				<!-- Footer -->
			{% include 'footer.twig' %}
				<!-- ./Footer -->
			</div>
			<!-- ./Page -->
		</div>
		<!-- ./Main -->
		<div class="screenRight"></div>
		<!-- Scripts -->
		{% include 'scripts.twig' %}
		<!-- ./Scripts -->
	</body>
	<!-- ./Body -->
</html>
<!-- ./Html -->

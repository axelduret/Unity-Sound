<!DOCTYPE html>

<!-- Html -->
<html
	lang="en">

	<!-- Head -->
	<head>

		<!-- Meta Datas -->
		<meta charset="UTF-8">
		<meta
		name="viewport" content="width=device-width, initial-scale=1.0">

		<!-- Favicon Svg -->
		<link
		rel="icon" type="image/svg+xml" href="favicon.svg">

		<!-- Page Title -->
		<title>Unity Sound</title>

		<!-- Recaptcha Scripts -->
		<script src="https://www.google.com/recaptcha/api.js"></script>

		<!-- Google Fonts Stylesheet -->
		<link
		rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Lobster&family=Dosis&family=Lato&family=Roboto">

		<!-- Main Compiled Stylesheet -->
		<link rel="stylesheet" href="css/styles.css">

	</head>
	<!-- ./Head -->

	<!-- Body -->
	<body>

		<!-- Page -->
		<div
			class="page">

			<!-- Header -->
			<div class="header-outterDiv shadow">

				<header class="header">

					<div
						class="header-innerDiv">

						<!-- Logo -->
						<div class="header-innerContent1">

							<img src="img/logo.svg" alt="Unity Sound" class="header-logoImg">

						</div>
						<!-- ./Logo -->

						<!-- Back to Home -->
						<div class="header-innerContent2" title="Back to Home" onclick="window.location='index.php'">

							- Welcome to Unity Sound Website -

						</div>
						<!-- ./Back to Home -->

						<!-- Blank -->
						<div class="header-innerContent3"></div>
						<!-- ./Blank -->

						<!-- Social Medias -->
						<div
							class="header-innerContent4">

							<!-- Youtube -->
							<div class="social-logo" onclick="window.open('https://www.youtube.com/channel/UCf6nzX_927ZVTM6uP9KYX7w');">

								<i class="fab fa-youtube-square fa-2x" aria-hidden="true" role="button" aria-label="Youtube" title="Youtube"></i>

								<span class="sr-only">Youtube</span>

							</div>
							<!-- ./Youtube -->

							<!-- Twitter -->
							<div class="social-logo" onclick="window.open('https://twitter.com/unity_sound');">

								<i class="fab fa-twitter-square fa-2x" aria-hidden="true" role="button" aria-label="Twitter" title="Twitter"></i>

								<span class="sr-only">Twitter</span>

							</div>
							<!-- ./Twitter -->

							<!-- Facebook -->
							<div class="social-logo" onclick="window.open('https://facebook.com/unitysoundsystem');">

								<i class="fab fa-facebook-square fa-2x" aria-hidden="true" role="button" aria-label="Facebook" title="Facebook"></i>

								<span class="sr-only">Facebook</span>

							</div>
							<!-- ./Facebook -->

						</div>
						<!-- ./Social Medias -->

					</div>

				</header>

			</div>
			<!-- ./Header -->

			<!-- Main Content -->
			{% block content %}{% endblock %}
			<!-- ./Main Content -->

			<!-- Footer -->
			<div class="footer-outterDiv shadow">

				<footer class="footer">

					<div class="footer-innerDiv">

						<div class="footer-innerContent" title="Axel Web Dev" onclick="window.open('https://axelwebdev.blogspot.com/');">

							AWD - 2020 &copy;

						</div>

					</footer>

				</div>

			</div>
			<!-- ./Footer -->

		</div>
		<!-- ./Page -->

		<!-- Fontawesome Icons Scripts -->
		<script src="js/fontawesome.min.js"></script>

		<!-- Jquerry Scripts -->
		<script src="js/jquery.min.js"></script>

		<!-- PopperJs Scripts -->
		<script src="js/popper.min.js"></script>

		<!-- Bootstrap Scripts -->
		<script src="js/bootstrap.min.js"></script>

		<!-- Custom Scripts -->
		<script src="js/scripts.js"></script>

	</body>
	<!-- ./Body -->

</html>
<!-- ./Html -->
